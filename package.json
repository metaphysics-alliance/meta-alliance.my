{
  "name": "meta-alliance-website",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:next dev:vite",
    "assets:roadmap": "node scripts/export-roadmap.mjs",
    "predev": "npm run assets:roadmap",
    "dev:next": "next dev -p 3000",
    "dev:vite": "vite --port 5173",
    "build": "npm run assets:roadmap && vite build",
    "preview": "vite preview --port 4173",
    "pricing:seed": "node scripts/seed-pricing.mjs",
    "pricing:rate": "node scripts/pull-exchange-rate.mjs",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:browserstack": "playwright test --config=playwright.browserstack.config.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=9999",
    "env:setup": "node scripts/setup-env.mjs",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "cf:deploy": "npx wrangler deploy --config cloudflare/worker/wrangler.toml",
    "cf:dev": "npx wrangler dev --config cloudflare/worker/wrangler.toml",
    "cf:kv": "npx wrangler kv:namespace create RATE_LIMIT --config cloudflare/worker/wrangler.toml"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.78.0",
    "lucide-react": "^0.441.0",
    "next": "^14.2.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/node": "24.6.2",
    "@types/react": "19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3",
    "vite": "^7.1.7"
  }
}
